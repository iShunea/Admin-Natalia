# Vite build-time environment variables
# Copy this file to .env and fill in your values

# Base path for router - admin panel accessible at /admin route
VITE_APP_BASE_NAME=/admin

# Backend API URL (backend runs on port 7500)
VITE_APP_API_URL=http://localhost:7500

# Optional: Application version
VITE_APP_VERSION=v9.2.1

# Optional: Google Maps API Key (for address autocomplete)
VITE_APP_GOOGLE_MAPS_API_KEY=

# Optional: Mapbox Access Token (for map visualization)
VITE_APP_MAPBOX_ACCESS_TOKEN=
